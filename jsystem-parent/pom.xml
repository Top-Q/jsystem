	<build>
		<!--<finalName>${project.artifactId}</finalName> -->
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.5.1</version>
				<configuration>
					<target>1.7</target>
					<source>1.7</source>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>2.2</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<modules>
		<module>../jsystem-core-projects</module>
		<module>../jsystem-core-system-objects</module>
		<module>../jsystem-archetypes</module>
		<module>../jsystem-miscellaneous/jsystem-maven-plugin</module>
		<module>../jsystem-miscellaneous/jsystem-services-so</module>
		<module>../jsystem-miscellaneous/jsystem-services-tests</module>
	</modules>
	<repositories>
		<repository>
			<id>topq</id>
			<url>${topq.repository.rootUrl}/content/groups/public</url>
		</repository>
	</repositories>
	<distributionManagement>
		<repository>
			<id>topq</id>
			<name>RepositoryProxy</name>
			<url>${topq.repository.rootUrl}/content/repositories/releases</url>
		</repository>
		<snapshotRepository>
			<id>topq</id>
			<name>RepositoryProxy</name>
			<url>${topq.repository.rootUrl}/content/repositories/snapshots</url>
		</snapshotRepository>
	</distributionManagement>
	<pluginRepositories>
		<pluginRepository>
			<id>topq</id>
			<url>${topq.repository.rootUrl}/content/groups/public</url>
		</pluginRepository>
	</pluginRepositories>
	<profiles>
		<profile>
			<id>dist</id>
			<modules>
				<module>../jsystem-assembly/jsystem-runner</module>
			</modules>
		</profile>
	</profiles>
</project>
